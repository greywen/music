{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "lib": [
            "es2015",
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "target": "esnext",
        "isolatedModules": false,
        "noEmit": false
    },
    "include": [
        "server.ts",
        "routes/**/*.ts"
    ]
}